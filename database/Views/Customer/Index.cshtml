@model IEnumerable<database.Models.Customer>
@{
}
<link href="~/css/site.css" rel="stylesheet" />
<table class="table table-bordered">
    <tr>
        <th  class="aa">
            Name
        </th>
        <th>
            Mobole
        </th>
        <th>
            Email
        </th>
        <th>
            LocationId
        </th>
    </tr>


    @foreach (var Customer in Model)
    {
        <tr>
            <td>
                @Customer.Name
            </td>

            <td>
                @Customer.Mobole
            </td>
            <td>
                @Customer.Email
            </td>
            <td>
                @Customer.LocationId
            </td>


        </tr>
    }
</table>